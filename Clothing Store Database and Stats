CREATE TABLE clothing (id integer primary key, item text, quantity integer, aisle text, price integer);
insert into clothing values
(1, 'shorts', 40, 1, 1.50),
(2, 't shirts', 30, 2, 2.00),
(3, 'pants', 20, 3, 4.00),
(4, 'dress', 15, 4, 2.50),
(5, 'tank top', 27, 5, 4.75);
select * from clothing;
select * from clothing order by price asc;
